# =============================================================================
# DHARMA CALENDAR - Git Ignore
# =============================================================================

# -----------------------------------------------------------------------------
# Dependencies
# -----------------------------------------------------------------------------
node_modules/
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# -----------------------------------------------------------------------------
# Build outputs
# -----------------------------------------------------------------------------
/.next/
/out/
/build/
/dist/

# -----------------------------------------------------------------------------
# Generated files
# -----------------------------------------------------------------------------
/src/generated/
*.tsbuildinfo

# -----------------------------------------------------------------------------
# Environment files
# -----------------------------------------------------------------------------
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
# Note: .env.example should be committed

# -----------------------------------------------------------------------------
# Cache
# -----------------------------------------------------------------------------
.turbo/
.cache/

# -----------------------------------------------------------------------------
# Testing
# -----------------------------------------------------------------------------
/coverage/

# -----------------------------------------------------------------------------
# IDE & Editors
# -----------------------------------------------------------------------------
.idea/
.vscode/
*.swp
*.swo
*.sublime-project
*.sublime-workspace

# -----------------------------------------------------------------------------
# OS files
# -----------------------------------------------------------------------------
.DS_Store
Thumbs.db
Desktop.ini

# -----------------------------------------------------------------------------
# Debug logs
# -----------------------------------------------------------------------------
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# -----------------------------------------------------------------------------
# Misc
# -----------------------------------------------------------------------------
*.pem
.vercel

# -----------------------------------------------------------------------------
# Project specific
# -----------------------------------------------------------------------------
/backups/
/_dev/

# Temporary development files in root
/*.log
/database-export.json
